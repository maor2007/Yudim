package yudim;
import java.util.Scanner;

class Shiur3{
	
	public static void f1()
	{
		Scanner scan = new Scanner(System.in);
		
		int num;
		
		System.out.println("enter number: ");
		num = scan.nextInt();
		
		System.out.println("number divide by 5: " + num / 5);
		
	}

	public static void main(String[] args) 
	{
		
		f1();
		
	}


}


//shiur 5 sheela 3

package yudim;
import java.util.Scanner;

class Shiur3{
	
	public static int f1(int num1, int num2)
	{
		Scanner scan = new Scanner(System.in);
		
		int num3;
		
		num3 = num1 * num2;
		
		return num3;
				
	}

	public static void main(String[] args) 
	{
			
		System.out.println("number: " + f1(5,7));
		
	}


}


//shiur 5 sheela 4

package yudim;
import java.util.Scanner;

class Shiur3{
	
	public static double f1(int num1, int num2, int num3)
	{
		Scanner scan = new Scanner(System.in);
		
		int divide=0;
		
		System.out.println("number 1: " + num1);
		System.out.println("number 2: " + num2);
		System.out.println("number 3: " + num3);
	
		divide =num1 + num2 + num3;
		divide /= 3;
		
		return divide;
				
	}

	public static void main(String[] args) 
	{
			
		System.out.println("the divide: " + (f1(100,100,100)));
		
	}


}


//shiur 5 sheela 5

package yudim;
import java.util.Scanner;

class Shiur3{
	
	public static void f1(int radius)
	{
		Scanner scan = new Scanner(System.in);
		
		double area;
		double circumference;
		
		circumference = (2 * 3.14 * radius);
		area = radius * radius * 3.14;
		
		System.out.println("circumference: " + circumference);
		System.out.println("area: " + area);
				
	}

	public static void main(String[] args) 
	{
			
		f1(5);
		
	}


}


//shiur 5 sheela 6

package yudim;
import java.util.Scanner;

class Shiur3{
	
	public static void f1(int line1, int line2)
	{
		Scanner scan = new Scanner(System.in);
		
		double line3;
		
		line3 = (line1 * line1) + (line2 * line2);
		line3 = Math.sqrt(line3);
		
		System.out.println("line 3: " + line3);
				
	}

	public static void main(String[] args) 
	{		
		f1(5,5);
	}


}


