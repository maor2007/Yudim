package yudim;
import java.util.Scanner;

class Shiur3{
	
	public static void f1(char str1, char str2, char str3)
	{
		Scanner scan = new Scanner(System.in);
		
		System.out.println(str3+""+str2+""+str1);
				
	}

	public static void main(String[] args) 
	{		
		
		f1('a','b','c');
	}


}


//shiur 5 sheela 8

package yudim;
import java.util.Scanner;

class Shiur3{
	
	public static int f1(int math1, int efresh, int eivar)
	{
		Scanner scan = new Scanner(System.in);
		
		int An = math1 + (efresh - 1) * eivar;
		
		return An;
	}

	public static void main(String[] args) 
	{		
		
		System.out.println("An: " + f1(0,5,2));
	}


}
