package yudim;
import java.util.Scanner;

class Shiur3{
	
	public static int f1(int math1, int efresh, int eivar)
	{
		Scanner scan = new Scanner(System.in);
		
		int An = math1 + (efresh - 1) * eivar;
		
		return An;
	}

	public static void main(String[] args) 
	{		
		
		System.out.println("An: " + f1(0,5,2));
	}


}


//shiur 6 sheela 1

import java.util.Scanner;

class main{

    public static void f1()
    {
        Scanner scan = new Scanner(System.in);

        int i;
        int sum = 0;

        System.out.println("enter 5 grades: ");
        for(i = 0; i < 5; i++){
            int grade = scan.nextInt();
            sum += grade;
        }
        System.out.println("average: " + sum / 5);
    }

    public static void main(String[] args)
    {

        f1();
    }
    
}


//shiur 6 sheela 2

import java.util.Scanner;

class main{

    public static void f1()
    {
        Scanner scan = new Scanner(System.in);

        boolean i = true;
        String str;
        int sum = 0;
        int check = 0;

        System.out.println("enter strings until you press enter: ");

        do {

            str = scan.nextLine();
            sum += 1;
            
        }while(!(str.equals("enter")));

        System.out.println("number of words: " + sum);
    }

    public static void main(String[] args)
    {

        f1();
    }

}


//shiur 6 sheela 3

import java.util.Scanner;

class main{

    public static void f1(int code)
    {
        Scanner scan = new Scanner(System.in);

        int sum = 0;
        int attempts = 3;

        System.out.println("enter a code: ");
        do {
            sum = scan.nextInt();

            if(sum == code) {
                System.out.println("how much money? ");
                attempts = -5;
            }

            else {
                attempts -= 1;
                System.out.println("WRONG! you have " + attempts + " more tries:");
            }


        }while(attempts > 0);

        if(attempts == 0)
            System.out.println("error");

    }

    public static void main(String[] args)
    {

        f1(1234);
    }

}


//shiur 6 sheela 4

class main{

    public static void f1(int i)
    {

        int temp;
        int sum = 0;

        while(i != 0) {
            temp = i % 10;
            sum += 1;
            i = i / 10;
        }

        System.out.println("number of digits: " + sum);
    }

    public static void main(String[] args)
    {

        f1(123);
    }

}


//shiur 6 sheela 5

class main{

    public static int f1(int i)
    {
        
        int temp;
        int sum = 0;

        while(i != 0) {
            temp = i % 10;
            sum += temp;
            i = i / 10;
        }

        return sum;
    }

    public static void main(String[] args)
    {
        System.out.println("sum: " + f1(123));
    }

}


//shiur 6 sheela 6


class main{

    public static void f1(String str)
    {

        int i = 0;
        int reverse = 0;
        int temp = 0;
        int yes = 1;

        reverse = str.length();
        temp = reverse / 2;

        for(i = 0; i < reverse / 2; i++,temp--)
        {
            if(str.charAt(i) == str.charAt(temp))
            {
                yes++;
            }

        }

        if(yes == reverse / 2)
        {
            System.out.println("yes");
        }
        else
        {
            System.out.println("no");
        }

    }

    public static void main(String[] args)
    {
        f1("aba");
    }

}


//shiur 6 sheela 7

import java.util.Scanner;

class main{

    public static void f1(int num)
    {
        Scanner scan = new Scanner(System.in);

        boolean first = true;
        boolean last = true;
        int i = 0;

        for(i = 1; i <= num; i++)
        {
            if(num % i ==  0){
                if (!first || !last)
                    System.out.print(",");
                System.out.print(i);
                first = false;
            }
        }

    }

    public static void main(String[] args)
    {
        f1(24);
    }

}


//shiur 6 sheela 8



class main{

    public static void f1()
    {
        int i = 0;
        int j = 0;

        for(i = 1; i <= 10; i++)
        {
            for(j = 1; j <= 10; j++)
                System.out.print(i * j + " ");
            System.out.println();
        }
        
    }

    public static void main(String[] args)
    {

        f1();
    }

}


//shiur 6 sheela 9

import java.util.Scanner;

class main{

    public static void f1()
    {
        Scanner scan = new Scanner(System.in);

        int count = 0;
        int max = 0;
        int min = 100;
        int i;

        System.out.println("enter 7 grades: ");

        while (count <= 7) {
            int num = scan.nextInt();
            if (num > max)
                max = num;

            if (num < min)
                min = num;

            count++;
        }

        System.out.println("highest grade: " + max);
        System.out.println("lowest grade: " + min);

    }

    public static void main(String[] args)
    {

        f1();
    }

}


//shiur 6 sheela 10

class main{

    public static boolean f1(int num)
    {

        boolean b = false;
        int num1 = 1;
        int num2 = 1;
        int temp = 0;

        while(temp < 800 & num1 < 800 & num2 < 800)
        {
            temp = num1 + num2;

            if(num == temp)
                return true;

            num1 = num2 + temp;

            if(num == num1)
                return true;

            num2 = num1 + temp;

            if(num == num2)
                return true;

        }

        return false;

    }

    public static void main(String[] args)
    {
        System.out.println(f1(34));
    }

}


//shiur 6 sheela 11





































//מבחן 1
// שאלה 1

package yudim;
import java.util.Scanner;

class Shiur3{
	

	public static void f1(){
		
		Scanner scan = new Scanner(System.in);
		int bigBro;
		System.out.println("enter big brother age: ");
		bigBro = scan.nextInt();
		
		System.out.println("big bro: " + bigBro);
		System.out.println("little bro: " + (bigBro - 10));
		System.out.println("big sister: " + (bigBro - 9));
		System.out.println("little sister: " + (bigBro - 6));
		
		
	}

	
	public static void main(String[] args) 
	{
		f1();
		
	}


}


//שאלה 2


package yudim;
import java.util.Scanner;

class Shiur3{
	

	public static void f1(){
		
		Scanner scan = new Scanner(System.in);
		int num;
		
		System.out.println("enter 3 digit number: ");
		num = scan.nextInt();
		
		int digit1 = num % 10;
        int digit2 = (num / 10) % 10;
        int digit3 = num / 100;

        int reversed = digit1 * 100 + digit2 * 10 + digit3;
		
		System.out.println(reversed);
		
		
	}

	
	public static void main(String[] args) 
	{
		f1();
		
	}


}


//שאלה 3

package yudim;
import java.util.Scanner;

class Shiur3{
	

	public static void f1(){
		
		Scanner scan = new Scanner(System.in);
		String str1;
		String str2;
		
		
		System.out.println("enter 2 strings: ");
		str1 = scan.next();
		str2 = scan.next();
		
		if(str1.length() > str2.length())
			System.out.println(str2);
		else
			System.out.println(str1);
		
		String str3 = str2.substring(0, 3);
		String str4 = str1.substring(0, 3);
		System.out.println(str3 + " " + str4);
		
		if(str1.compareTo(str2) < 0)
			System.out.println(str2);
		else
			System.out.println(str1);
		
	}

	
	public static void main(String[] args) 
	{
		f1();
		
	}


}


