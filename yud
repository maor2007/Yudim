	public static void f1()
	{
		Scanner scan = new Scanner(System.in);
		
		String name;
		
		System.out.println("enter your name: ");
		name = scan.next();
		
		System.out.println("hello " + name);
		
	}

	public static void main(String[] args) 
	{
		
		f1();
		
	}


}


