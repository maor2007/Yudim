import java.util.Scanner;

class main{

    public static void f1(int num1, int num2)
    {
        int result = 0;
        int i = 0;

        while (i < num1)
        {
            result += num2;
            i++;
        }

        System.out.println(result);


    }

    public static void main(String[] args)
    {
        f1(2, 6);
    }

}


//shiur 6 sheela 12

import java.util.Scanner;

class main{

    public static void f1(int height)
    {
        int i;
        int j;
        int base = height + (height - 1);
        int x1 = base / 2;
        int x2 = base / 2;

        for(i = 0; i <= height; i++)
        {
            for(j = 0; j < base; j++)
            {
                if((j > x1) && (j < x2))
                    System.out.print("*");
                else
                    System.out.print(" ");
            }
            System.out.println();
            x1--;
            x2++;
        }

    }

    public static void main(String[] args)
    {
        f1(3);
    }

}


