import java.util.Scanner;

class main{

    public static void f1(int start, int mana)
    {
        int i = 0;
        int sum = 0;

        for(i = 0; i <= 10; i++)
        {
            start = start * mana;
            System.out.println(start);
        }

    }

    public static void main(String[] args)
    {
        f1(3, 3);
    }

}


//shiur 6 sheela 17 

class main{

    public static void f1(int mash, int month)
    {
        double ribit = (mash / month) * 0.05;
        double avv = (mash / month) + ribit;
        int i;

        for(i = 0; i <= month; i++)
        {
            mash = (int) (mash + ribit);
        }

        System.out.println("average monthly payment: " + avv);
        System.out.println("all payments: " + mash);

    }

    public static void main(String[] args)
    {
        f1(5000, 10);
    }

}

//shiur 6 sheela 18

import java.util.Scanner;

class main{

    public static void f1(int num)
    {
        int i = 0;
        int sum = 0;
        String temp = ("");
        String binary = ("");

        while(num > 0)
        {
            sum = num % 2;
            temp += sum;
            num /= 2;
        }

        for(i = temp.length() - 1; i >= 0; i--)
        {
            binary += temp.charAt(i);
        }
        System.out.println(binary);

    }

    public static void main(String[] args)
    {
        f1(11);
    }

}


//shiur 6 sheela 19

import java.util.Scanner;
import java.util.Random;

class main{

    public static void f1()
    {
        Random r = new Random();
        int x1, x2, x3, x4, x5, y1, y2, y3, y4, y5, z1, z2, z3, z4, z5, sum, i;

        x1 = r.nextInt(100)+1;
        x2 = r.nextInt(100)+1;
        x3 = r.nextInt(100)+1;
        x4 = r.nextInt(100)+1;
        x5 = r.nextInt(100)+1;
        y1 = r.nextInt(100)+1;
        y2 = r.nextInt(100)+1;
        y3 = r.nextInt(100)+1;
        y4 = r.nextInt(100)+1;
        y5 = r.nextInt(100)+1;

        z1 = x1 + y1;
        z2 = x2 + y2;
        z3 = x3 + y3;
        z4 = x4 + y4;
        z5 = x5 + y5;

        System.out.println("(" + x1 + "," + y1 + ")");
        System.out.println("(" + x2 + "," + y2 + ")");
        System.out.println("(" + x3 + "," + y3 + ")");
        System.out.println("(" + x4 + "," + y4 + ")");
        System.out.println("(" + x5 + "," + y5 + ")");


    }

    public static void main(String[] args)
    {
        f1();
    }

}

