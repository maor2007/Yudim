class main{

    public static void f1(int a1 , int d , int n)
    {

        int i = 0;

        for(i = 1; i <= n; i++)
        {
            System.out.println(a1);
            a1 += d;
        }

    }

    public static void main(String[] args)
    {
        f1(0, 5, 5);
    }

}


//shiur 6 sheela 14

class main{

    public static void f1(int first, int last)
    {

        int i = last - first;
        int num1 = 1;
        int num2 = 1;
        int temp = 0;

        while(i < last)
        {
            temp = num1 + num2;
            if(i > first && i <= last)
                System.out.println(temp);

            i++;

            num1 = num2 + temp;
            if(i > first && i <= last)
                System.out.println(num1);

            i++;

            num2 = num1 + temp;
            if(i > first && i <= last)
                System.out.println(num2);

            i++;

        }

    }

    public static void main(String[] args)
    {
        f1(2, 5);
    }

}


//shiur 6 sheela 15

import java.util.Scanner;

class main{

    public static void f1(int height)
    {
        int i;
        int j;
        int base = height * height + 1;
        int x1 = height - 1;
        int x2 = height + 1;

        for(i = 0; i < height; i++)
        {
            for(j = 0; j < base; j++)
            {
                if (j >= x1 && j <= x2)
                    System.out.print("*");
                else
                    System.out.print(" ");
            }
            System.out.println();
            x1--;
            x2++;
        }

    }

    public static void main(String[] args)
    {
        f1(7);
    }

}


