//shiur 2
//sheela 1

package yudim;

public class Shiur3 
{
	public static void main(String[] args) 
	{
		
		int x = 1;
		int y = 2;
		int z = 3;
		
		System.out.println(x);
		System.out.println(y);
		System.out.println(z);
	
	
	
	}


}





//sheela 2 shiur 2

package yudim;

public class Shiur3 
{
	public static void main(String[] args) 
	{
		
		int x = 25;
		float y = (float)63.5;
		
		System.out.println(x +"   "+ y);
	
	
	
	}


}


//sheela 3 shiur 2

package yudim;


public class Shiur3 
{
	public static void main(String[] args) 
	{
		
		char x = 'm';
		char y = 'a';
		char z = 'o';
		char a = 'r';
		
		System.out.print(x);
        System.out.print(y);
        System.out.print(z);
        System.out.print(a);
	
	}


}


