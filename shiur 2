package yudim;


public class Shiur3 
{
	public static void main(String[] args) 
	{
		
		int x = 10;
		int y = 15;
		
		System.out.println(x+" is less then "+y);
	
	}


}


//shiur 2 sheela 5

package yudim;


public class Shiur3 
{
	public static void main(String[] args) 
	{
		
		int x = 10;
		int y = 15;
		int z = 25;
		float result = (float)(x+y+z)/3;
		
		System.out.println(result);
		
	
	}


}


//shiur 2 sheela 7

package yudim;


public class Shiur3 
{
	public static void main(String[] args) 
	{
		
		int x = 5;
		int y;
		y=x * 2 + 5;
		
		System.out.println("x="+x+"  y="+y);
	}


}
